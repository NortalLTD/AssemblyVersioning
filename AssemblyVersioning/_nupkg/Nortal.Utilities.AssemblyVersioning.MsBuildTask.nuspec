		<file src="build\*.*" target="build\" />
		<file src="tools\*.*" target="tools\" />
		<file src="content\Properties\*.*" target="content\Properties\" />
		<file src="..\bin\$configuration$\Nortal.Utilities.AssemblyVersioning.dll" target="build\" />
		<file src="..\bin\$configuration$\Nortal.Utilities.TextTemplating.dll" target="build\" />
